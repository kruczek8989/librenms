    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of lease active (option 53 with value 13) packets
         transmited."
    ::= { dhcpSnoopingStatisticsInterfaceEntry 27 }

dhcpSnoopingMibConformance OBJECT IDENTIFIER
    ::= { dhcpSnoopingMib 2 }

dhcpSnoopingMibCompliances OBJECT IDENTIFIER
    ::= { dhcpSnoopingMibConformance 1 }

dhcpSnoopingMibGroups OBJECT IDENTIFIER
    ::= { dhcpSnoopingMibConformance 2 }

dhcpSnoopingConfigGlobalsInfoGroup OBJECT-GROUP
    OBJECTS     { dhcpSnoopingConfigGlobalsMode }
    STATUS      current
    DESCRIPTION
        "A collection of objects suitable for bulk operations."
    ::= { dhcpSnoopingMibGroups 1 }

dhcpSnoopingConfigInterfaceInfoGroup OBJECT-GROUP
    OBJECTS     { dhcpSnoopingConfigInterfaceTrustMode }
    STATUS      current
    DESCRIPTION
        "A collection of objects suitable for bulk operations."
    ::= { dhcpSnoopingMibGroups 2 }

dhcpSnoopingStatusAssignedIpTableInfoGroup OBJECT-GROUP
    OBJECTS     { dhcpSnoopingStatusAssignedIpIfIndex,
                  dhcpSnoopingStatusAssignedIpIpAddress,
                  dhcpSnoopingStatusAssignedIpNetmask,
                  dhcpSnoopingStatusAssignedIpDhcpServerIp }
    STATUS      current
    DESCRIPTION
