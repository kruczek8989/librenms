    ::= { dhcpSnoopingMibGroups 3 }

dhcpSnoopingControlInterfaceClearStatisticsTableInfoGroup OBJECT-GROUP
    OBJECTS     {                   dhcpSnoopingControlInterfaceClearStatisticsClear }
    STATUS      current
    DESCRIPTION
        "A collection of objects suitable for bulk operations."
    ::= { dhcpSnoopingMibGroups 4 }

dhcpSnoopingStatisticsInterfaceTableInfoGroup OBJECT-GROUP
    OBJECTS     { dhcpSnoopingStatisticsInterfaceRxDiscover,
                  dhcpSnoopingStatisticsInterfaceRxOffer,
                  dhcpSnoopingStatisticsInterfaceRxRequest,
                  dhcpSnoopingStatisticsInterfaceRxDecline,
                  dhcpSnoopingStatisticsInterfaceRxAck,
                  dhcpSnoopingStatisticsInterfaceRxNak,
                  dhcpSnoopingStatisticsInterfaceRxRelease,
                  dhcpSnoopingStatisticsInterfaceRxInform,
