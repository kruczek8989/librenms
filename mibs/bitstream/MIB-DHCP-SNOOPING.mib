-- *****************************************************************
-- MIB-DHCP-SNOOPING: BitStream ENT private MIB
-- ****************************************************************

MIB-DHCP-SNOOPING DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    switchMgmt FROM BTS-SMI
    Integer32 FROM SNMPv2-SMI
    IpAddress FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    MacAddress FROM SNMPv2-TC
    TruthValue FROM SNMPv2-TC
    InterfaceIndex FROM BTS-TC
    ;

dhcpSnoopingMib MODULE-IDENTITY
    LAST-UPDATED "202503200000Z"
    ORGANIZATION
        "BitStream Sp. z o.o."
    CONTACT-INFO
        "www.bitstream.pl"
    DESCRIPTION
        "This is a private version of the DHCP Snooping MIB"
    REVISION    "201410100000Z"
    DESCRIPTION
        "Editorial changes"
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { switchMgmt 56 }


dhcpSnoopingMibObjects OBJECT IDENTIFIER
    ::= { dhcpSnoopingMib 1 }

dhcpSnoopingConfig OBJECT IDENTIFIER
    ::= { dhcpSnoopingMibObjects 2 }

dhcpSnoopingConfigGlobals OBJECT IDENTIFIER
    ::= { dhcpSnoopingConfig 1 }

dhcpSnoopingConfigGlobalsMode OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates the DHCP snooping mode operation. Possible modes are - true:
         Enable DHCP snooping mode operation. When DHCP snooping mode operation
         is enabled, the DHCP request messages will be forwarded to trusted
         ports and only allow reply packets from trusted ports. false: Disable
         DHCP snooping mode operation."
    ::= { dhcpSnoopingConfigGlobals 1 }

dhcpSnoopingConfigInterfaceTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DhcpSnoopingConfigInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table of DHCP Snooping port configuration parameters"
    ::= { dhcpSnoopingConfig 2 }

dhcpSnoopingConfigInterfaceEntry OBJECT-TYPE
    SYNTAX      DhcpSnoopingConfigInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each port has a set of parameters"
    INDEX       { dhcpSnoopingConfigInterfaceIfIndex }
    ::= { dhcpSnoopingConfigInterfaceTable 1 }

DhcpSnoopingConfigInterfaceEntry ::= SEQUENCE {
    dhcpSnoopingConfigInterfaceIfIndex    InterfaceIndex,
    dhcpSnoopingConfigInterfaceTrustMode  TruthValue
}

dhcpSnoopingConfigInterfaceIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Logical interface number of the physical port."
    ::= { dhcpSnoopingConfigInterfaceEntry 1 }

dhcpSnoopingConfigInterfaceTrustMode OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Indicates the DHCP snooping port mode. Possible port modes are - true:
         Configures the port as trusted source of the DHCP messages. false:
         Configures the port as untrusted source of the DHCP messages."
    ::= { dhcpSnoopingConfigInterfaceEntry 2 }

dhcpSnoopingStatus OBJECT IDENTIFIER
    ::= { dhcpSnoopingMibObjects 3 }

dhcpSnoopingStatusAssignedIpTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DhcpSnoopingStatusAssignedIpEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table of assigned IP information in DHCP Snooping "
    ::= { dhcpSnoopingStatus 1 }

dhcpSnoopingStatusAssignedIpEntry OBJECT-TYPE
    SYNTAX      DhcpSnoopingStatusAssignedIpEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry has a set of parameters"
    INDEX       { dhcpSnoopingStatusAssignedIpMacAddress,
                  dhcpSnoopingStatusAssignedIpVlanId }
    ::= { dhcpSnoopingStatusAssignedIpTable 1 }

DhcpSnoopingStatusAssignedIpEntry ::= SEQUENCE {
    dhcpSnoopingStatusAssignedIpMacAddress    MacAddress,
    dhcpSnoopingStatusAssignedIpVlanId        Integer32,
    dhcpSnoopingStatusAssignedIpIfIndex       InterfaceIndex,
    dhcpSnoopingStatusAssignedIpIpAddress     IpAddress,
    dhcpSnoopingStatusAssignedIpNetmask       IpAddress,
    dhcpSnoopingStatusAssignedIpDhcpServerIp  IpAddress
}

dhcpSnoopingStatusAssignedIpMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "MAC address."
    ::= { dhcpSnoopingStatusAssignedIpEntry 1 }

dhcpSnoopingStatusAssignedIpVlanId OBJECT-TYPE
    SYNTAX      Integer32 (1..4095)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The VLAN id of the VLAN."
    ::= { dhcpSnoopingStatusAssignedIpEntry 2 }

dhcpSnoopingStatusAssignedIpIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Logical interface number of the physical port of the DHCP client."
    ::= { dhcpSnoopingStatusAssignedIpEntry 3 }

dhcpSnoopingStatusAssignedIpIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "IP address assigned to DHCP client by DHCP server."
    ::= { dhcpSnoopingStatusAssignedIpEntry 4 }

dhcpSnoopingStatusAssignedIpNetmask OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Netmask assigned to DHCP client by DHCP server."
    ::= { dhcpSnoopingStatusAssignedIpEntry 5 }

dhcpSnoopingStatusAssignedIpDhcpServerIp OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "IP address of the DHCP server that assigns the IP address and netmask."
    ::= { dhcpSnoopingStatusAssignedIpEntry 6 }

dhcpSnoopingControl OBJECT IDENTIFIER
    ::= { dhcpSnoopingMibObjects 4 }

dhcpSnoopingControlInterfaceClearStatisticsTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DhcpSnoopingControlInterfaceClearStatisticsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table to clear port statistics in DHCP Snooping"
    ::= { dhcpSnoopingControl 1 }

dhcpSnoopingControlInterfaceClearStatisticsEntry OBJECT-TYPE
    SYNTAX      DhcpSnoopingControlInterfaceClearStatisticsEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each port has a set of parameters"
    INDEX       {                   dhcpSnoopingControlInterfaceClearStatisticsIfIndex }
    ::= { dhcpSnoopingControlInterfaceClearStatisticsTable 1 }

DhcpSnoopingControlInterfaceClearStatisticsEntry ::= SEQUENCE {
    dhcpSnoopingControlInterfaceClearStatisticsIfIndex  InterfaceIndex,
    dhcpSnoopingControlInterfaceClearStatisticsClear    TruthValue
}

dhcpSnoopingControlInterfaceClearStatisticsIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Logical interface number of the physical port."
    ::= { dhcpSnoopingControlInterfaceClearStatisticsEntry 1 }

dhcpSnoopingControlInterfaceClearStatisticsClear OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "clear statistics per physical port."
    ::= { dhcpSnoopingControlInterfaceClearStatisticsEntry 2 }

dhcpSnoopingStatistics OBJECT IDENTIFIER
    ::= { dhcpSnoopingMibObjects 5 }

dhcpSnoopingStatisticsInterfaceTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DhcpSnoopingStatisticsInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table of port statistics in DHCP Snooping "
    ::= { dhcpSnoopingStatistics 2 }

dhcpSnoopingStatisticsInterfaceEntry OBJECT-TYPE
    SYNTAX      DhcpSnoopingStatisticsInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry has a set of parameters"
    INDEX       { dhcpSnoopingStatisticsInterfaceIfIndex }
    ::= { dhcpSnoopingStatisticsInterfaceTable 1 }

DhcpSnoopingStatisticsInterfaceEntry ::= SEQUENCE {
    dhcpSnoopingStatisticsInterfaceIfIndex             InterfaceIndex,
    dhcpSnoopingStatisticsInterfaceRxDiscover          Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxOffer             Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxRequest           Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxDecline           Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxAck               Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxNak               Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxRelease           Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxInform            Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxLeaseQuery        Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxLeaseUnassigned   Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxLeaseUnknown      Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxLeaseActive       Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxDiscardChksumErr  Unsigned32,
    dhcpSnoopingStatisticsInterfaceRxDiscardUntrust    Unsigned32,
    dhcpSnoopingStatisticsInterfaceTxDiscover          Unsigned32,
    dhcpSnoopingStatisticsInterfaceTxOffer             Unsigned32,
    dhcpSnoopingStatisticsInterfaceTxRequest           Unsigned32,
    dhcpSnoopingStatisticsInterfaceTxDecline           Unsigned32,
    dhcpSnoopingStatisticsInterfaceTxAck               Unsigned32,
    dhcpSnoopingStatisticsInterfaceTxNak               Unsigned32,
    dhcpSnoopingStatisticsInterfaceTxRelease           Unsigned32,
    dhcpSnoopingStatisticsInterfaceTxInform            Unsigned32,
    dhcpSnoopingStatisticsInterfaceTxLeaseQuery        Unsigned32,
    dhcpSnoopingStatisticsInterfaceTxLeaseUnassigned   Unsigned32,
    dhcpSnoopingStatisticsInterfaceTxLeaseUnknown      Unsigned32,
    dhcpSnoopingStatisticsInterfaceTxLeaseActive       Unsigned32
}

dhcpSnoopingStatisticsInterfaceIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Logical interface number of the physical port."
    ::= { dhcpSnoopingStatisticsInterfaceEntry 1 }

dhcpSnoopingStatisticsInterfaceRxDiscover OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of discover (option 53 with value 1) packets received."
    ::= { dhcpSnoopingStatisticsInterfaceEntry 2 }

dhcpSnoopingStatisticsInterfaceRxOffer OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of offer (option 53 with value 2) packets received."
    ::= { dhcpSnoopingStatisticsInterfaceEntry 3 }

dhcpSnoopingStatisticsInterfaceRxRequest OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of request (option 53 with value 3) packets received."
    ::= { dhcpSnoopingStatisticsInterfaceEntry 4 }

dhcpSnoopingStatisticsInterfaceRxDecline OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "The number of decline (option 53 with value 4) packets received."
