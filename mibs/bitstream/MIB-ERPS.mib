erpsStatus OBJECT IDENTIFIER
    ::= { erpsMibObjects 3 }

erpsStatusTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF ErpsStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table contains status per EPRS group."
    ::= { erpsStatus 1 }

erpsStatusEntry OBJECT-TYPE
    SYNTAX      ErpsStatusEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Status."
    INDEX       { erpsStatusGroupIndex }
    ::= { erpsStatusTable 1 }

ErpsStatusEntry ::= SEQUENCE {
    erpsStatusGroupIndex                   Integer32,
    erpsStatusActive                       TruthValue,
    erpsStatusProtectionState              ErpsProtectionState,
    erpsStatusRplBlocked                   TruthValue,
    erpsStatusWtrRemaining                 Unsigned32,
    erpsStatusAdminCmd                     ErpsAdminCmd,
    erpsStatusFopAlarm                     TruthValue,
    erpsStatusTxActive                     TruthValue,
    erpsStatusTxRequestOrState             ErpsRequestState,
    erpsStatusTxRplBlocked                 TruthValue,
    erpsStatusTxDoNotFlush                 TruthValue,
    erpsStatusBlockedPortReference         ErpsPort,
    erpsStatusPort0Blocked                 TruthValue,
    erpsStatusPort0State                   ErpsPortState,
    erpsStatusPort0RxActive                TruthValue,
    erpsStatusPort0RxRequestOrState        ErpsRequestState,
    erpsStatusPort0RxRplBlocked            TruthValue,
    erpsStatusPort0RxDoNotFlush            TruthValue,
    erpsStatusPort0RxBlockedPortReference  ErpsPort,
    erpsStatusPort0RxNodeId                OCTET STRING,
    erpsStatusPort1Blocked                 TruthValue,
    erpsStatusPort1State                   ErpsPortState,
    erpsStatusPort1RxActive                TruthValue,
    erpsStatusPort1RxRequestOrState        ErpsRequestState,
    erpsStatusPort1RxRplBlocked            TruthValue,
    erpsStatusPort1RxDoNotFlush            TruthValue,
    erpsStatusPort1RxBlockedPortReference  ErpsPort,
    erpsStatusPort1RxNodeId                OCTET STRING
}

erpsStatusGroupIndex OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "ERPS group index number. Valid range is (1..max groups). The maximum
         group number is platform-specific and can be retrieved from the ERPS
         capabilities."
    ::= { erpsStatusEntry 1 }

erpsStatusActive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies whether ERPS group is currently active or not."
    ::= { erpsStatusEntry 2 }

erpsStatusProtectionState OBJECT-TYPE
    SYNTAX      ErpsProtectionState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies current ERPS group protection state."
    ::= { erpsStatusEntry 3 }

erpsStatusRplBlocked OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies whether the RPL is currently blocked or not."
    ::= { erpsStatusEntry 4 }

erpsStatusWtrRemaining OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the remaining wait-time before restoring ring."
    ::= { erpsStatusEntry 5 }

erpsStatusAdminCmd OBJECT-TYPE
    SYNTAX      ErpsAdminCmd
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the currently active administrative command."
    ::= { erpsStatusEntry 6 }

erpsStatusFopAlarm OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies whether the FailureOfProtocol alarm is currently active or
         not."
    ::= { erpsStatusEntry 7 }

erpsStatusTxActive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies whether R-APS transmission is currently active."
    ::= { erpsStatusEntry 8 }

erpsStatusTxRequestOrState OBJECT-TYPE
    SYNTAX      ErpsRequestState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the currently transmitted request/state. Only relevant when
         Tx is active."
    ::= { erpsStatusEntry 9 }

erpsStatusTxRplBlocked OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the currently transmitted RPL Blocked flag. Only relevant
         when Tx is active."
    ::= { erpsStatusEntry 10 }

erpsStatusTxDoNotFlush OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the currently transmitted Do Not Flush flag. Only relevant
         when Tx is active."
    ::= { erpsStatusEntry 11 }

erpsStatusBlockedPortReference OBJECT-TYPE
    SYNTAX      ErpsPort
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the currently transmitted Blocked Port Reference. Only
         relevant when Tx is active."
    ::= { erpsStatusEntry 12 }

erpsStatusPort0Blocked OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies whether ring Port 0 is blocked or not."
    ::= { erpsStatusEntry 13 }

erpsStatusPort0State OBJECT-TYPE
    SYNTAX      ErpsPortState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the state of ring Port 0."
    ::= { erpsStatusEntry 14 }

erpsStatusPort0RxActive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies if ring Port0 receives R-APS."
    ::= { erpsStatusEntry 15 }

erpsStatusPort0RxRequestOrState OBJECT-TYPE
    SYNTAX      ErpsRequestState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the most recently received request/state on ring Port 0. Only
         relevant when Rx is active."
    ::= { erpsStatusEntry 16 }

erpsStatusPort0RxRplBlocked OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the most recently received RPL Blocked flag on ring Port 0.
         Only relevant when Rx is active."
    ::= { erpsStatusEntry 17 }

erpsStatusPort0RxDoNotFlush OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the most recently received Do Not Flush flag on ring Port 0.
         Only relevant when Rx is active."
    ::= { erpsStatusEntry 18 }

erpsStatusPort0RxBlockedPortReference OBJECT-TYPE
    SYNTAX      ErpsPort
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the most recently received Blocked Port Reference on ring
         Port 0. Only relevant when Rx is active."
    ::= { erpsStatusEntry 19 }

erpsStatusPort0RxNodeId OBJECT-TYPE
    SYNTAX      OCTET STRING (SIZE(6))
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the most recently received node ID on ring Port 0. Only
         relevant when Rx is active."
    ::= { erpsStatusEntry 20 }

erpsStatusPort1Blocked OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies whether ring Port 1 is blocked or not."
    ::= { erpsStatusEntry 21 }

erpsStatusPort1State OBJECT-TYPE
    SYNTAX      ErpsPortState
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies the state of ring Port 1."
    ::= { erpsStatusEntry 22 }

erpsStatusPort1RxActive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Specifies if ring Port1 receives R-APS."
    ::= { erpsStatusEntry 23 }

erpsStatusPort1RxRequestOrState OBJECT-TYPE
    SYNTAX      ErpsRequestState
    MAX-ACCESS  read-only
