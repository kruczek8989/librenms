-- *****************************************************************
-- MIB-ERPS: BitStream ENT private MIB
-- ****************************************************************

MIB-ERPS DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    switchMgmt FROM BTS-SMI
    Counter64 FROM SNMPv2-SMI
    Integer32 FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    TruthValue FROM SNMPv2-TC
    InterfaceIndex FROM BTS-TC
    RowEditorState FROM BTS-TC
    VlanListQuarter FROM BTS-TC
    ;

erpsMib MODULE-IDENTITY
    LAST-UPDATED "202503200000Z"
    ORGANIZATION
        "BitStream Sp. z o.o."
    CONTACT-INFO
        "www.bitstream.pl"
    DESCRIPTION
        "Private MIB for Ethernet Ring Protection Switching, ERPS."
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { switchMgmt 72 }


ErpsAdminCmd ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the Administrative command."
    SYNTAX      INTEGER { manualSwitch(1), forcedSwitch(2), clear(3) }

ErpsControlCmd ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies a control command."
    SYNTAX      INTEGER { none(0), admCmdForcedSwitch(1),
                          admCmdManualSwitch(2), admCmdClear(3),
                          statisticsClear(4) }

ErpsPort ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies a particular logical ring port."
    SYNTAX      INTEGER { port0(1), port1(2) }

ErpsPortState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the ring port state."
    SYNTAX      INTEGER { ok(1), signalFail(2) }

ErpsProtectionState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the ring protection state."
    SYNTAX      INTEGER { none(1), idle(2), protected(3),
                          forcedSwitch(4), manualSwitch(5),
                          pending(6) }

ErpsRequestState ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies a request/state."
    SYNTAX      INTEGER { none(1), manualSwitch(2), signalFail(3),
                          forcedSwitch(4), event(5) }

ErpsRingType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the ERPS ring type."
    SYNTAX      INTEGER { major(1), sub(2) }

ErpsRplMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the Ring Protection Link mode. Use 'none' if port is neither
         RPL owner nor neighbour."
    SYNTAX      INTEGER { none(1), owner(2), neighbour(3) }

ErpsVersion ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "Specifies the ERPS protocol version."
    SYNTAX      INTEGER { version1(1), version2(2) }

erpsMibObjects OBJECT IDENTIFIER
    ::= { erpsMib 1 }

erpsCapabilities OBJECT IDENTIFIER
    ::= { erpsMibObjects 1 }

erpsCapabilitiesMaxGroups OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Maximum number of configured ERPS groups."
    ::= { erpsCapabilities 1 }

erpsCapabilitiesMaxVlansPerGroup OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Maximum number of protected VLANS per ERPS group."
    ::= { erpsCapabilities 2 }

erpsConfig OBJECT IDENTIFIER
    ::= { erpsMibObjects 2 }

erpsConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF ErpsConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the ERPS group configuration table."
    ::= { erpsConfig 1 }

erpsConfigEntry OBJECT-TYPE
    SYNTAX      ErpsConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents an ERPS group."
    INDEX       { erpsConfigGroupIndex }
    ::= { erpsConfigTable 1 }

ErpsConfigEntry ::= SEQUENCE {
    erpsConfigGroupIndex                       Integer32,
    erpsConfigRingType                         ErpsRingType,
    erpsConfigPort0                            InterfaceIndex,
    erpsConfigPort1                            InterfaceIndex,
    erpsConfigInterconnectMajorRingGroupIndex  Unsigned32,
    erpsConfigVirtualChannel                   TruthValue,
    erpsConfigPort0SignalFailMepIndex          Unsigned32,
    erpsConfigPort0ApsMepIndex                 Unsigned32,
    erpsConfigPort1SignalFailMepIndex          Unsigned32,
    erpsConfigPort1ApsMepIndex                 Unsigned32,
    erpsConfigHoldOffTime                      Unsigned32,
    erpsConfigWaitToRestoreTime                Unsigned32,
    erpsConfigGuardTime                        Unsigned32,
    erpsConfigRplMode                          ErpsRplMode,
    erpsConfigRplPort                          ErpsPort,
    erpsConfigRevertive                        TruthValue,
    erpsConfigVersion                          ErpsVersion,
    erpsConfigTopologyChange                   TruthValue,
    erpsConfigProtectedVlans0Kto1K             VlanListQuarter,
    erpsConfigProtectedVlans1Kto2K             VlanListQuarter,
    erpsConfigProtectedVlans2Kto3K             VlanListQuarter,
    erpsConfigProtectedVlans3Kto4K             VlanListQuarter,
    erpsConfigAction                           RowEditorState
}

erpsConfigGroupIndex OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "ERPS group index number. Valid range is (1..max groups). The maximum
         group number is platform-specific and can be retrieved from the ERPS
         capabilities."
    ::= { erpsConfigEntry 1 }

erpsConfigRingType OBJECT-TYPE
    SYNTAX      ErpsRingType
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Type of ring. Can only be set once for a ring instance."
    ::= { erpsConfigEntry 2 }

erpsConfigPort0 OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "ifindex of ring protection port 0. Can only be set once for a ring
         instance."
    ::= { erpsConfigEntry 3 }

erpsConfigPort1 OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "ifindex of ring protection Port 1. For interconnected sub-rings this
         value must be zero. Can only be set once for a ring instance."
    ::= { erpsConfigEntry 4 }

erpsConfigInterconnectMajorRingGroupIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "For sub-ring: zero = not interconnected; > zero = index of major ring
         group. For major ring: zero = not interconnected; > zero = is
         interconnected (i.e flag-like semantics). Can only be set once for a
         ring instance."
    ::= { erpsConfigEntry 5 }

erpsConfigVirtualChannel OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Whether to use a virtual channel. Can only be set once for a ring
         instance."
    ::= { erpsConfigEntry 6 }

erpsConfigPort0SignalFailMepIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Index of SignalFail MEP for Port 0. Zero if not set. To clear an ERPS
         group's MEP association, set this to zero; it affects all MEP indices."
    ::= { erpsConfigEntry 7 }

erpsConfigPort0ApsMepIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Index of APS MEP for Port 0. Zero if not set."
    ::= { erpsConfigEntry 8 }

erpsConfigPort1SignalFailMepIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Index of SignalFail MEP for Port 1. Zero if not set. Must be zero for
         interconnected sub-rings."
    ::= { erpsConfigEntry 9 }

erpsConfigPort1ApsMepIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Index of APS MEP for Port 1. Zero if not set. Must be zero for
         interconnected sub-rings with virtual channel."
    ::= { erpsConfigEntry 10 }

erpsConfigHoldOffTime OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Hold off time in ms. Value is rounded down to 100ms precision. Valid
         range is 0-10000 ms"
    ::= { erpsConfigEntry 11 }

erpsConfigWaitToRestoreTime OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Wait-to-Restore time in ms. Valid range is 60000-720000 ms (1-12
         minutes)."
    ::= { erpsConfigEntry 12 }

erpsConfigGuardTime OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Guard time in ms. Valid range is 10-2000 ms."
    ::= { erpsConfigEntry 13 }

erpsConfigRplMode OBJECT-TYPE
    SYNTAX      ErpsRplMode
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Ring Protection Link mode."
    ::= { erpsConfigEntry 14 }

erpsConfigRplPort OBJECT-TYPE
    SYNTAX      ErpsPort
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "RPL port. Only valid if the ERPS group is RPL owner or neighbour."
    ::= { erpsConfigEntry 15 }

erpsConfigRevertive OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Revertive (true) or Non-revertive (false) mode."
    ::= { erpsConfigEntry 16 }

erpsConfigVersion OBJECT-TYPE
    SYNTAX      ErpsVersion
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "ERPS protocol version."
    ::= { erpsConfigEntry 17 }

erpsConfigTopologyChange OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Whether to enable Topology Change propagation; only valid for an
         interconnected node."
    ::= { erpsConfigEntry 18 }

erpsConfigProtectedVlans0Kto1K OBJECT-TYPE
    SYNTAX      VlanListQuarter
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "First quarter of bit-array indicating whether a VLAN is protected by
         this ring instance ('1') or not ('0')."
    ::= { erpsConfigEntry 19 }

erpsConfigProtectedVlans1Kto2K OBJECT-TYPE
    SYNTAX      VlanListQuarter
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Second quarter of bit-array indicating whether a VLAN is protected by
         this ring instance ('1') or not ('0')."
    ::= { erpsConfigEntry 20 }

erpsConfigProtectedVlans2Kto3K OBJECT-TYPE
    SYNTAX      VlanListQuarter
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Third quarter of bit-array indicating whether a VLAN is protected by
         this ring instance ('1') or not ('0')."
    ::= { erpsConfigEntry 21 }

erpsConfigProtectedVlans3Kto4K OBJECT-TYPE
    SYNTAX      VlanListQuarter
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Fourth quarter of bit-array indicating whether a VLAN is protected by
         this ring instance ('1') or not ('0')."
    ::= { erpsConfigEntry 22 }

erpsConfigAction OBJECT-TYPE
    SYNTAX      RowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action"
    ::= { erpsConfigEntry 100 }

erpsConfigRowEditor OBJECT IDENTIFIER
