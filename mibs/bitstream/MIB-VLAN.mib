vlanStatusMembershipsVlanVlanId OBJECT-TYPE
    SYNTAX      Vlan
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "VLAN ID. Valid range is 1 - 4095."
    ::= { vlanStatusMembershipsVlanEntry 1 }

vlanStatusMembershipsVlanVlanUser OBJECT-TYPE
    SYNTAX      VlanUserType
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
