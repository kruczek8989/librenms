    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Next-hop address."
    ::= { ipConfigRoutesIpv6RowEditor 3 }

ipConfigRoutesIpv6RowEditorNextHopInterface OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "If the next-hop address is a link-local address, then the interface of
         the link-local address must be specified here. Otherwise this value is
         not used."
    ::= { ipConfigRoutesIpv6RowEditor 4 }

ipConfigRoutesIpv6RowEditorAction OBJECT-TYPE
    SYNTAX      RowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action"
    ::= { ipConfigRoutesIpv6RowEditor 100 }

ipStatus OBJECT IDENTIFIER
    ::= { ipMibObjects 3 }

ipStatusGlobals OBJECT IDENTIFIER
    ::= { ipStatus 1 }

ipStatusGlobalsIpv4NeighbourTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IpStatusGlobalsIpv4NeighbourEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the IPv4 neighbour table (ARP)."
    ::= { ipStatusGlobals 1 }

ipStatusGlobalsIpv4NeighbourEntry OBJECT-TYPE
    SYNTAX      IpStatusGlobalsIpv4NeighbourEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents an entry in the underlying operting
         system's ARP table."
    INDEX       { ipStatusGlobalsIpv4NeighbourIpv4 }
    ::= { ipStatusGlobalsIpv4NeighbourTable 1 }

IpStatusGlobalsIpv4NeighbourEntry ::= SEQUENCE {
    ipStatusGlobalsIpv4NeighbourIpv4        IpAddress,
    ipStatusGlobalsIpv4NeighbourMacAddress  MacAddress,
    ipStatusGlobalsIpv4NeighbourInterface   InterfaceIndex
}

ipStatusGlobalsIpv4NeighbourIpv4 OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "IPv4 address."
    ::= { ipStatusGlobalsIpv4NeighbourEntry 1 }

ipStatusGlobalsIpv4NeighbourMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "MAC address associated with the IP address"
    ::= { ipStatusGlobalsIpv4NeighbourEntry 2 }

ipStatusGlobalsIpv4NeighbourInterface OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Interface the neighbour can be reached on."
    ::= { ipStatusGlobalsIpv4NeighbourEntry 3 }

ipStatusGlobalsIpv6NeighbourTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IpStatusGlobalsIpv6NeighbourEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the IPv6 neighbour table."
    ::= { ipStatusGlobals 2 }

ipStatusGlobalsIpv6NeighbourEntry OBJECT-TYPE
    SYNTAX      IpStatusGlobalsIpv6NeighbourEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents an entry in the underlying operting
         system's nieghbour table."
    INDEX       { ipStatusGlobalsIpv6NeighbourIpAddress,
                  ipStatusGlobalsIpv6NeighbourInterfaceQuery }
    ::= { ipStatusGlobalsIpv6NeighbourTable 1 }

IpStatusGlobalsIpv6NeighbourEntry ::= SEQUENCE {
    ipStatusGlobalsIpv6NeighbourIpAddress       InetAddressIPv6,
    ipStatusGlobalsIpv6NeighbourInterfaceQuery  Integer32,
    ipStatusGlobalsIpv6NeighbourMacAddress      MacAddress,
    ipStatusGlobalsIpv6NeighbourInterface       InterfaceIndex
}

ipStatusGlobalsIpv6NeighbourIpAddress OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "IP address."
    ::= { ipStatusGlobalsIpv6NeighbourEntry 1 }

ipStatusGlobalsIpv6NeighbourInterfaceQuery OBJECT-TYPE
    SYNTAX      Integer32 (0..2147483647)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "If 'ipAddress' is a link-local address, then the interface index where
         the host can be reached must be specified here, otherwise set this to
         zero."
    ::= { ipStatusGlobalsIpv6NeighbourEntry 2 }

ipStatusGlobalsIpv6NeighbourMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "MAC address associated with the IP address"
    ::= { ipStatusGlobalsIpv6NeighbourEntry 3 }

ipStatusGlobalsIpv6NeighbourInterface OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Interface the neighbour can be reached on."
    ::= { ipStatusGlobalsIpv6NeighbourEntry 4 }

ipStatusInterfaces OBJECT IDENTIFIER
    ::= { ipStatus 2 }

ipStatusInterfacesLinkTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IpStatusInterfacesLinkEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table provides link-layer status information for IP interfaces."
    ::= { ipStatusInterfaces 1 }

ipStatusInterfacesLinkEntry OBJECT-TYPE
    SYNTAX      IpStatusInterfacesLinkEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents an IP interface."
    INDEX       { ipStatusInterfacesLinkIfIndex }
    ::= { ipStatusInterfacesLinkTable 1 }

IpStatusInterfacesLinkEntry ::= SEQUENCE {
    ipStatusInterfacesLinkIfIndex           InterfaceIndex,
    ipStatusInterfacesLinkOsInterfaceIndex  Unsigned32,
    ipStatusInterfacesLinkMtu               Unsigned32,
    ipStatusInterfacesLinkMacAddress        MacAddress,
    ipStatusInterfacesLinkUp                TruthValue,
    ipStatusInterfacesLinkBroadcast         TruthValue,
    ipStatusInterfacesLinkLoopback          TruthValue,
    ipStatusInterfacesLinkRunning           TruthValue,
    ipStatusInterfacesLinkNoarp             TruthValue,
    ipStatusInterfacesLinkPromisc           TruthValue,
    ipStatusInterfacesLinkMulticast         TruthValue
}

ipStatusInterfacesLinkIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Interface index number."
    ::= { ipStatusInterfacesLinkEntry 1 }

ipStatusInterfacesLinkOsInterfaceIndex OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Interface index used by the operating system."
    ::= { ipStatusInterfacesLinkEntry 2 }

ipStatusInterfacesLinkMtu OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "MTU for the interface."
    ::= { ipStatusInterfacesLinkEntry 3 }

ipStatusInterfacesLinkMacAddress OBJECT-TYPE
    SYNTAX      MacAddress
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "MAC-address of the interface."
    ::= { ipStatusInterfacesLinkEntry 4 }

ipStatusInterfacesLinkUp OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Indicates if the interface is up."
    ::= { ipStatusInterfacesLinkEntry 5 }

ipStatusInterfacesLinkBroadcast OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Indicates if the interface is capable of transmitting broadcast
         traffic."
    ::= { ipStatusInterfacesLinkEntry 6 }

ipStatusInterfacesLinkLoopback OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Indicates if the interface is a loop-back interface."
    ::= { ipStatusInterfacesLinkEntry 7 }

ipStatusInterfacesLinkRunning OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Interface is running (according to the operating system)."
    ::= { ipStatusInterfacesLinkEntry 8 }

ipStatusInterfacesLinkNoarp OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Indicates if the interface will answer to ARP requests."
    ::= { ipStatusInterfacesLinkEntry 9 }

ipStatusInterfacesLinkPromisc OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Indicates if the interface is in promisc mode."
    ::= { ipStatusInterfacesLinkEntry 10 }

ipStatusInterfacesLinkMulticast OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION
        "Indicates if the interface supports multicast."
    ::= { ipStatusInterfacesLinkEntry 11 }

ipStatusInterfacesIpv4Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF IpStatusInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table provides IPv4 status information for IP interfaces. If an
         interface is configured to use a DHCP client, then the address can be
         found here."
    ::= { ipStatusInterfaces 2 }

ipStatusInterfacesIpv4Entry OBJECT-TYPE
    SYNTAX      IpStatusInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
