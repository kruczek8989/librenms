ipConfigInterfaces OBJECT IDENTIFIER
    ::= { ipConfig 2 }

ipConfigInterfacesTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IpConfigInterfacesEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the IP interface table. When an IP interface is created it can
         be configured in the other tables found in this MIB."
    ::= { ipConfigInterfaces 1 }

ipConfigInterfacesEntry OBJECT-TYPE
    SYNTAX      IpConfigInterfacesEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Entries in this table represent IP interfaces created on the system"
    INDEX       { ipConfigInterfacesIfIndex }
    ::= { ipConfigInterfacesTable 1 }

IpConfigInterfacesEntry ::= SEQUENCE {
    ipConfigInterfacesIfIndex  InterfaceIndex,
    ipConfigInterfacesAction   RowEditorState
}

ipConfigInterfacesIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Interface index number."
    ::= { ipConfigInterfacesEntry 1 }

ipConfigInterfacesAction OBJECT-TYPE
    SYNTAX      RowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action"
    ::= { ipConfigInterfacesEntry 100 }

ipConfigInterfacesTableRowEditor OBJECT IDENTIFIER
    ::= { ipConfigInterfaces 2 }

ipConfigInterfacesTableRowEditorIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Interface index number."
    ::= { ipConfigInterfacesTableRowEditor 1 }

ipConfigInterfacesTableRowEditorAction OBJECT-TYPE
    SYNTAX      RowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action"
    ::= { ipConfigInterfacesTableRowEditor 100 }

ipConfigInterfacesIpv4Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF IpConfigInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "IPv4 interface configuration table. This table enables IPv4 related
         configuration of the corresponding IP interface."
    ::= { ipConfigInterfaces 3 }

ipConfigInterfacesIpv4Entry OBJECT-TYPE
    SYNTAX      IpConfigInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents an IP interface."
    INDEX       { ipConfigInterfacesIpv4IfIndex }
    ::= { ipConfigInterfacesIpv4Table 1 }

IpConfigInterfacesIpv4Entry ::= SEQUENCE {
    ipConfigInterfacesIpv4IfIndex                    InterfaceIndex,
    ipConfigInterfacesIpv4Active                     TruthValue,
    ipConfigInterfacesIpv4EnableDhcpClient           TruthValue,
    ipConfigInterfacesIpv4Ipv4Address                IpAddress,
    ipConfigInterfacesIpv4PrefixSize                 Unsigned32,
    ipConfigInterfacesIpv4DhcpClientFallbackTimeout  Unsigned32
}

ipConfigInterfacesIpv4IfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Interface index number."
    ::= { ipConfigInterfacesIpv4Entry 1 }

ipConfigInterfacesIpv4Active OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable IPv4.
         
         IPv4 can only be enabled if either the DHCP client is enabled, or a
         valid address has been configured."
    ::= { ipConfigInterfacesIpv4Entry 2 }

ipConfigInterfacesIpv4EnableDhcpClient OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable IPv4 DHCP client.
         
         Note: the DHCP client can only be enabled if there is no conflict in
         the values of: ipv4Address, prefixSize, and dhcpClientFallbackTimeout."
    ::= { ipConfigInterfacesIpv4Entry 3 }

ipConfigInterfacesIpv4Ipv4Address OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "IPv4 address.
         
         Note: Multiple interfaces may not have overlapping networks."
    ::= { ipConfigInterfacesIpv4Entry 4 }

ipConfigInterfacesIpv4PrefixSize OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Prefix size of the network.
         
         Note: Multiple interfaces may not have overlapping networks."
    ::= { ipConfigInterfacesIpv4Entry 5 }

ipConfigInterfacesIpv4DhcpClientFallbackTimeout OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "DHCP client fallback timer.
         
         If DHCP is disabled then this object has no effect. If DHCP is enabled
         and the fallback timeout value is different from zero, then this timer
         will stop the DHCP process and assign the ipv4Address to the interface
         instead."
    ::= { ipConfigInterfacesIpv4Entry 6 }

ipConfigInterfacesIpv6Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF IpConfigInterfacesIpv6Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "IPv6 interface configuration table. This table enables IPv6 related
         configuration of the corresponding IP interface."
    ::= { ipConfigInterfaces 4 }

ipConfigInterfacesIpv6Entry OBJECT-TYPE
    SYNTAX      IpConfigInterfacesIpv6Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents an IP interface."
    INDEX       { ipConfigInterfacesIpv6IfIndex }
    ::= { ipConfigInterfacesIpv6Table 1 }

IpConfigInterfacesIpv6Entry ::= SEQUENCE {
    ipConfigInterfacesIpv6IfIndex      InterfaceIndex,
    ipConfigInterfacesIpv6Active       TruthValue,
    ipConfigInterfacesIpv6Ipv6Address  InetAddressIPv6,
    ipConfigInterfacesIpv6PrefixSize   Unsigned32
}

ipConfigInterfacesIpv6IfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Interface index number."
    ::= { ipConfigInterfacesIpv6Entry 1 }

ipConfigInterfacesIpv6Active OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable the static configured IPv6 address.
         
         The static configured IPv6 address can only be configured if a valid
         address has been written into 'ipv6Address' and 'prefixSize'."
    ::= { ipConfigInterfacesIpv6Entry 2 }

ipConfigInterfacesIpv6Ipv6Address OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Static configured IPv6 address."
    ::= { ipConfigInterfacesIpv6Entry 3 }

ipConfigInterfacesIpv6PrefixSize OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Prefix size of the network."
    ::= { ipConfigInterfacesIpv6Entry 4 }

ipConfigRoutes OBJECT IDENTIFIER
    ::= { ipConfig 3 }

ipConfigRoutesIpv4Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF IpConfigRoutesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the IPv4 route configuration table."
    ::= { ipConfigRoutes 1 }

ipConfigRoutesIpv4Entry OBJECT-TYPE
    SYNTAX      IpConfigRoutesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents a configured route.
         
         Note: a route may be configured without being active."
    INDEX       { ipConfigRoutesIpv4NetworkAddress,
                  ipConfigRoutesIpv4NetworkPrefixSize,
                  ipConfigRoutesIpv4NextHop }
    ::= { ipConfigRoutesIpv4Table 1 }

IpConfigRoutesIpv4Entry ::= SEQUENCE {
    ipConfigRoutesIpv4NetworkAddress     IpAddress,
    ipConfigRoutesIpv4NetworkPrefixSize  Integer32,
    ipConfigRoutesIpv4NextHop            IpAddress,
    ipConfigRoutesIpv4Action             RowEditorState
}

ipConfigRoutesIpv4NetworkAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Network address."
    ::= { ipConfigRoutesIpv4Entry 1 }

ipConfigRoutesIpv4NetworkPrefixSize OBJECT-TYPE
    SYNTAX      Integer32 (0..32)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Network prefix size."
    ::= { ipConfigRoutesIpv4Entry 2 }

ipConfigRoutesIpv4NextHop OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Next-hop address."
    ::= { ipConfigRoutesIpv4Entry 3 }

ipConfigRoutesIpv4Action OBJECT-TYPE
    SYNTAX      RowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action"
    ::= { ipConfigRoutesIpv4Entry 100 }

ipConfigRoutesIpv4RowEditor OBJECT IDENTIFIER
    ::= { ipConfigRoutes 2 }

ipConfigRoutesIpv4RowEditorNetworkAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Network address."
    ::= { ipConfigRoutesIpv4RowEditor 1 }

ipConfigRoutesIpv4RowEditorNetworkPrefixSize OBJECT-TYPE
    SYNTAX      Integer32 (0..32)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Network prefix size."
    ::= { ipConfigRoutesIpv4RowEditor 2 }

ipConfigRoutesIpv4RowEditorNextHop OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Next-hop address."
    ::= { ipConfigRoutesIpv4RowEditor 3 }

ipConfigRoutesIpv4RowEditorAction OBJECT-TYPE
    SYNTAX      RowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action"
    ::= { ipConfigRoutesIpv4RowEditor 100 }

ipConfigRoutesIpv6Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF IpConfigRoutesIpv6Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the IPv6 route configuration table."
    ::= { ipConfigRoutes 3 }

ipConfigRoutesIpv6Entry OBJECT-TYPE
    SYNTAX      IpConfigRoutesIpv6Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry in this table represents a configured route.
         
         Note: a route may be configured without being active."
    INDEX       { ipConfigRoutesIpv6NetworkAddress,
                  ipConfigRoutesIpv6NetworkPrefixSize,
                  ipConfigRoutesIpv6NextHop,
                  ipConfigRoutesIpv6NextHopInterface }
    ::= { ipConfigRoutesIpv6Table 1 }

IpConfigRoutesIpv6Entry ::= SEQUENCE {
    ipConfigRoutesIpv6NetworkAddress     InetAddressIPv6,
    ipConfigRoutesIpv6NetworkPrefixSize  Integer32,
    ipConfigRoutesIpv6NextHop            InetAddressIPv6,
    ipConfigRoutesIpv6NextHopInterface   InterfaceIndex,
    ipConfigRoutesIpv6Action             RowEditorState
}

ipConfigRoutesIpv6NetworkAddress OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Network address."
    ::= { ipConfigRoutesIpv6Entry 1 }

ipConfigRoutesIpv6NetworkPrefixSize OBJECT-TYPE
    SYNTAX      Integer32 (0..128)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Network prefix size."
    ::= { ipConfigRoutesIpv6Entry 2 }

ipConfigRoutesIpv6NextHop OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Next-hop address."
    ::= { ipConfigRoutesIpv6Entry 3 }

ipConfigRoutesIpv6NextHopInterface OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "If the next-hop address is a link-local address, then the interface of
         the link-local address must be specified here. Otherwise this value is
         not used."
    ::= { ipConfigRoutesIpv6Entry 4 }

ipConfigRoutesIpv6Action OBJECT-TYPE
    SYNTAX      RowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action"
    ::= { ipConfigRoutesIpv6Entry 100 }

ipConfigRoutesIpv6RowEditor OBJECT IDENTIFIER
    ::= { ipConfigRoutes 4 }

ipConfigRoutesIpv6RowEditorNetworkAddress OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Network address."
    ::= { ipConfigRoutesIpv6RowEditor 1 }

ipConfigRoutesIpv6RowEditorNetworkPrefixSize OBJECT-TYPE
    SYNTAX      Integer32 (0..128)
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Network prefix size."
    ::= { ipConfigRoutesIpv6RowEditor 2 }

ipConfigRoutesIpv6RowEditorNextHop OBJECT-TYPE
