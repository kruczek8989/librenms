dhcpServerConfigExcludedIpTableRowEditorAction OBJECT-TYPE
    SYNTAX      RowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action"
    ::= { dhcpServerConfigExcludedIpTableRowEditor 100 }

dhcpServerConfigPoolTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DhcpServerConfigPoolEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table is DHCP server pool onfiguration table. The indexe is pool
         name."
    ::= { dhcpServerConfig 5 }

dhcpServerConfigPoolEntry OBJECT-TYPE
    SYNTAX      DhcpServerConfigPoolEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry has a set of parameters"
    INDEX       { dhcpServerConfigPoolPoolName }
    ::= { dhcpServerConfigPoolTable 1 }

DhcpServerConfigPoolEntry ::= SEQUENCE {
    dhcpServerConfigPoolPoolName               DisplayString,
    dhcpServerConfigPoolPoolType               DhcpServerPoolEnum,
    dhcpServerConfigPoolIpv4Address            IpAddress,
    dhcpServerConfigPoolSubnetMask             IpAddress,
    dhcpServerConfigPoolSubnetBroadcast        IpAddress,
    dhcpServerConfigPoolLeaseDay               Unsigned32,
    dhcpServerConfigPoolLeaseHour              Unsigned32,
    dhcpServerConfigPoolLeaseMinute            Unsigned32,
    dhcpServerConfigPoolDomainName             DisplayString,
    dhcpServerConfigPoolDefaultRouter1         IpAddress,
    dhcpServerConfigPoolDefaultRouter2         IpAddress,
    dhcpServerConfigPoolDefaultRouter3         IpAddress,
    dhcpServerConfigPoolDefaultRouter4         IpAddress,
    dhcpServerConfigPoolDnsServer1             IpAddress,
    dhcpServerConfigPoolDnsServer2             IpAddress,
    dhcpServerConfigPoolDnsServer3             IpAddress,
    dhcpServerConfigPoolDnsServer4             IpAddress,
    dhcpServerConfigPoolNtpServer1             IpAddress,
    dhcpServerConfigPoolNtpServer2             IpAddress,
    dhcpServerConfigPoolNtpServer3             IpAddress,
    dhcpServerConfigPoolNtpServer4             IpAddress,
    dhcpServerConfigPoolNetbiosNodeType        DhcpServerNetbiosNodeEnum,
    dhcpServerConfigPoolNetbiosScope           DisplayString,
    dhcpServerConfigPoolNetbiosNameServer1     IpAddress,
    dhcpServerConfigPoolNetbiosNameServer2     IpAddress,
    dhcpServerConfigPoolNetbiosNameServer3     IpAddress,
    dhcpServerConfigPoolNetbiosNameServer4     IpAddress,
    dhcpServerConfigPoolNisDomainName          DisplayString,
    dhcpServerConfigPoolNisServer1             IpAddress,
    dhcpServerConfigPoolNisServer2             IpAddress,
    dhcpServerConfigPoolNisServer3             IpAddress,
    dhcpServerConfigPoolNisServer4             IpAddress,
    dhcpServerConfigPoolClientIdentifierType   DhcpServerClientIdentifierEnum,
    dhcpServerConfigPoolClientIdentifierFqdn   DisplayString,
    dhcpServerConfigPoolClientIdentifierMac    MacAddress,
    dhcpServerConfigPoolClientHardwareAddress  MacAddress,
    dhcpServerConfigPoolClientName             DisplayString,
    dhcpServerConfigPoolVendorClassId1         DisplayString,
    dhcpServerConfigPoolVendorSpecificInfo1    DisplayString,
    dhcpServerConfigPoolVendorClassId2         DisplayString,
    dhcpServerConfigPoolVendorSpecificInfo2    DisplayString,
    dhcpServerConfigPoolVendorClassId3         DisplayString,
    dhcpServerConfigPoolVendorSpecificInfo3    DisplayString,
    dhcpServerConfigPoolVendorClassId4         DisplayString,
    dhcpServerConfigPoolVendorSpecificInfo4    DisplayString,
    dhcpServerConfigPoolAction                 RowEditorState
}

dhcpServerConfigPoolPoolName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Name of DHCP pool."
    ::= { dhcpServerConfigPoolEntry 1 }

dhcpServerConfigPoolPoolType OBJECT-TYPE
    SYNTAX      DhcpServerPoolEnum
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Type of pool. none(0) means the pool type is not defined yet.
         network(1) means the pool defines a pool of IP addresses to service
         more than one DHCP client. host(2) means the pool services for a
         specific DHCP client identified by client identifier or hardware
         address."
    ::= { dhcpServerConfigPoolEntry 2 }

dhcpServerConfigPoolIpv4Address OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Network number of the subnet. If the pool type is of network, the IP
         address can be any general IP address. If the pool type is of host, the
         IP address must be a unicast IP address."
    ::= { dhcpServerConfigPoolEntry 3 }

dhcpServerConfigPoolSubnetMask OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Subnet Mask. DHCP option 1. Specify subnet mask of the DHCP address
         pool, excluding 0.0.0.0 and 255.255.255.255."
    ::= { dhcpServerConfigPoolEntry 4 }

dhcpServerConfigPoolSubnetBroadcast OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Broadcast IP address in the subnet. DHCP option 28. Specify the
         broadcast address in use on the client's subnet."
    ::= { dhcpServerConfigPoolEntry 5 }

dhcpServerConfigPoolLeaseDay OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Number of days of lease time. DHCP option 51, 58 and 59. The value
         range is 0-365. Specify lease time that allows the client to request a
         lease time for the IP address. If all of LeaseDay, LeaseHour and
         LeaseMinute are 0's, then it means the lease time is infinite."
    ::= { dhcpServerConfigPoolEntry 6 }

dhcpServerConfigPoolLeaseHour OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Number of hours of lease time. DHCP option 51, 58 and 59. The value
         range is 0-23. Specify lease time that allows the client to request a
         lease time for the IP address. If all of LeaseDay, LeaseHour and
         LeaseMinute are 0's, then it means the lease time is infinite."
    ::= { dhcpServerConfigPoolEntry 7 }

dhcpServerConfigPoolLeaseMinute OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Number of minutes of lease time. DHCP option 51, 58 and 59. The value
         range is 0-59. Specify lease time that allows the client to request a
         lease time for the IP address. If all of LeaseDay, LeaseHour and
         LeaseMinute are 0's, then it means the lease time is infinite."
    ::= { dhcpServerConfigPoolEntry 8 }

dhcpServerConfigPoolDomainName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Domain name. DHCP option 15. Specify domain name that client should use
         when resolving hostname via DNS."
    ::= { dhcpServerConfigPoolEntry 9 }

dhcpServerConfigPoolDefaultRouter1 OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Default router 1."
    ::= { dhcpServerConfigPoolEntry 10 }

dhcpServerConfigPoolDefaultRouter2 OBJECT-TYPE
    SYNTAX      IpAddress
