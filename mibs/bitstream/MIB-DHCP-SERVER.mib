    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "High IP address."
    ::= { dhcpServerConfigExcludedEntry 2 }

dhcpServerConfigExcludedAction OBJECT-TYPE
    SYNTAX      RowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
