-- *****************************************************************
-- MIB-DHCP-SERVER: BitStream ENT private MIB
-- ****************************************************************

MIB-DHCP-SERVER DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    switchMgmt FROM BTS-SMI
    Integer32 FROM SNMPv2-SMI
    IpAddress FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    MacAddress FROM SNMPv2-TC
    TruthValue FROM SNMPv2-TC
    DisplayString FROM BTS-TC
    InterfaceIndex FROM BTS-TC
    RowEditorState FROM BTS-TC
    Unsigned16 FROM BTS-TC
    ;

dhcpServerMib MODULE-IDENTITY
    LAST-UPDATED "202503200000Z"
    ORGANIZATION
        "BitStream Sp. z o.o."
    CONTACT-INFO
        "www.bitstream.pl"
    DESCRIPTION
        "This is a private version of DhcpServer"
    REVISION    "201411270000Z"
    DESCRIPTION
        "revise descriptions by Palle"
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { switchMgmt 109 }


DhcpServerBindingEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of binding."
    SYNTAX      INTEGER { none(0), automatic(1), manual(2),
                          expired(3) }

DhcpServerBindingStateEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the state of binding."
    SYNTAX      INTEGER { none(0), allocated(1), committed(2),
                          expired(3) }

DhcpServerClientIdentifierEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of client identifier."
    SYNTAX      INTEGER { none(0), fqdn(1), mac(2) }

DhcpServerNetbiosNodeEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of NetBIOS node."
    SYNTAX      INTEGER { nodeNone(0), nodeB(1), nodeP(2), nodeM(3),
                          nodeH(4) }

DhcpServerPoolEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of DHCP pool."
    SYNTAX      INTEGER { none(0), network(1), host(2) }

dhcpServerMibObjects OBJECT IDENTIFIER
    ::= { dhcpServerMib 1 }

dhcpServerConfig OBJECT IDENTIFIER
    ::= { dhcpServerMibObjects 2 }

dhcpServerConfigGlobals OBJECT IDENTIFIER
    ::= { dhcpServerConfig 1 }

dhcpServerConfigGlobalsMode OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Global mode of DHCP server. true is to enable the functions of DHCP
         server and false is to disable it."
    ::= { dhcpServerConfigGlobals 1 }

dhcpServerConfigVlanTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DhcpServerConfigVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the table of DHCP server VLAN configuration. The index is VLAN
         ID."
    ::= { dhcpServerConfig 2 }

dhcpServerConfigVlanEntry OBJECT-TYPE
    SYNTAX      DhcpServerConfigVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each VLAN has a set of parameters"
    INDEX       { dhcpServerConfigVlanIfIndex }
    ::= { dhcpServerConfigVlanTable 1 }

DhcpServerConfigVlanEntry ::= SEQUENCE {
    dhcpServerConfigVlanIfIndex  InterfaceIndex,
    dhcpServerConfigVlanMode     TruthValue
}

dhcpServerConfigVlanIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Logical interface number of VLAN."
    ::= { dhcpServerConfigVlanEntry 1 }

dhcpServerConfigVlanMode OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "VLAN mode of DHCP server. true is to enable DHCP server per VLAN and
         false is to disable it per VLAN."
    ::= { dhcpServerConfigVlanEntry 2 }

dhcpServerConfigExcludedTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF DhcpServerConfigExcludedEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The table is DHCP server excluded IP onfiguration table. The indexes
         are low IP and high IP address."
    ::= { dhcpServerConfig 3 }

dhcpServerConfigExcludedEntry OBJECT-TYPE
    SYNTAX      DhcpServerConfigExcludedEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry has a set of parameters"
    INDEX       { dhcpServerConfigExcludedLowIpAddress,
                  dhcpServerConfigExcludedHighIpAddress }
    ::= { dhcpServerConfigExcludedTable 1 }

DhcpServerConfigExcludedEntry ::= SEQUENCE {
    dhcpServerConfigExcludedLowIpAddress   IpAddress,
    dhcpServerConfigExcludedHighIpAddress  IpAddress,
    dhcpServerConfigExcludedAction         RowEditorState
}

