    mstpStatusBridgeRootPort                 Unsigned32,
    mstpStatusBridgeMaxAge                   Unsigned32,
    mstpStatusBridgeForwardDelay             Unsigned32,
    mstpStatusBridgeBridgeMaxAge             Unsigned32,
    mstpStatusBridgeBridgeHelloTime          Unsigned32,
    mstpStatusBridgeBridgeForwardDelay       Unsigned32,
    mstpStatusBridgeTxHoldCount              Unsigned32,
    mstpStatusBridgeForceVersion             MSTPForceVersion,
    mstpStatusBridgeCistRegionalRoot         OCTET STRING,
