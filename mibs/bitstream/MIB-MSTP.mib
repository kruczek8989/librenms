mstpStatus OBJECT IDENTIFIER
    ::= { mstpMibObjects 3 }

mstpStatusBridgeTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF MstpStatusBridgeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This table represent the status of the bridge instances"
    ::= { mstpStatus 1 }

mstpStatusBridgeEntry OBJECT-TYPE
    SYNTAX      MstpStatusBridgeEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "A MSTP Bridge instance set of status objects"
    INDEX       { mstpStatusBridgeInstance }
    ::= { mstpStatusBridgeTable 1 }

MstpStatusBridgeEntry ::= SEQUENCE {
    mstpStatusBridgeInstance                 Integer32,
    mstpStatusBridgeBridgeId                 OCTET STRING,
