    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable/Disable the IPMC MVR global functionality."
    ::= { ipmcMvrConfigGlobals 1 }

ipmcMvrConfigPortTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IpmcMvrConfigPortEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table for managing extra MVR helper features per port basis."
    ::= { ipmcMvrConfig 2 }

ipmcMvrConfigPortEntry OBJECT-TYPE
    SYNTAX      IpmcMvrConfigPortEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each port has a set of parameters."
    INDEX       { ipmcMvrConfigPortPortIndex }
    ::= { ipmcMvrConfigPortTable 1 }

IpmcMvrConfigPortEntry ::= SEQUENCE {
    ipmcMvrConfigPortPortIndex         InterfaceIndex,
    ipmcMvrConfigPortDoImmediateLeave  TruthValue
}

ipmcMvrConfigPortPortIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Logical interface number of the physical port."
    ::= { ipmcMvrConfigPortEntry 1 }

ipmcMvrConfigPortDoImmediateLeave OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Enable/Disable the MVR immediate leave functionality."
    ::= { ipmcMvrConfigPortEntry 2 }

ipmcMvrConfigInterfaceTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IpmcMvrConfigInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table for managing MVR VLAN interface entries."
    ::= { ipmcMvrConfig 3 }

ipmcMvrConfigInterfaceEntry OBJECT-TYPE
    SYNTAX      IpmcMvrConfigInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Each entry has a set of parameters"
    INDEX       { ipmcMvrConfigInterfaceIfIndex }
    ::= { ipmcMvrConfigInterfaceTable 1 }

IpmcMvrConfigInterfaceEntry ::= SEQUENCE {
    ipmcMvrConfigInterfaceIfIndex               InterfaceIndex,
    ipmcMvrConfigInterfaceName                  DisplayString,
    ipmcMvrConfigInterfaceIgmpQuerierAddress    IpAddress,
    ipmcMvrConfigInterfaceMode                  IpmcMvrVlanInterfaceMode,
    ipmcMvrConfigInterfaceTagging               IpmcMvrVlanInterfaceTagging,
    ipmcMvrConfigInterfacePriority              Unsigned8,
    ipmcMvrConfigInterfaceLastListenerQueryInt  Unsigned32,
    ipmcMvrConfigInterfaceChannelProfile        DisplayString,
    ipmcMvrConfigInterfaceAction                RowEditorState
}

ipmcMvrConfigInterfaceIfIndex OBJECT-TYPE
    SYNTAX      InterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "Logical interface number of the VLAN interface."
    ::= { ipmcMvrConfigInterfaceEntry 1 }

ipmcMvrConfigInterfaceName OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0..16))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "MVR Name is an optional attribute to indicate the name of the specific
         MVR VLAN that user could easily associate the MVR VLAN purpose with its
         name."
    ::= { ipmcMvrConfigInterfaceEntry 2 }

ipmcMvrConfigInterfaceIgmpQuerierAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The static IPv4 source address of the specific MVR interface for seding
         IGMP Query message with respect to IGMP Querier election."
    ::= { ipmcMvrConfigInterfaceEntry 3 }

ipmcMvrConfigInterfaceMode OBJECT-TYPE
    SYNTAX      IpmcMvrVlanInterfaceMode
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "In Dynamic mode, MVR allows dynamic MVR membership reports on source
         ports. In Compatible mode, MVR membership reports are forbidden on
         source ports."
    ::= { ipmcMvrConfigInterfaceEntry 4 }

ipmcMvrConfigInterfaceTagging OBJECT-TYPE
    SYNTAX      IpmcMvrVlanInterfaceTagging
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Specify whether the traversed IGMP/MLD control frames will be sent as
         Untagged or Tagged with MVR VID."
    ::= { ipmcMvrConfigInterfaceEntry 5 }

ipmcMvrConfigInterfacePriority OBJECT-TYPE
    SYNTAX      Unsigned8
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Specify how the traversed IGMP/MLD control frames will be sent in
         prioritized manner in VLAN tag."
    ::= { ipmcMvrConfigInterfaceEntry 6 }

ipmcMvrConfigInterfaceLastListenerQueryInt OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "This setting Last Listener/Member Query Interval is used to control
         IGMP protocol stack for fast aging mechanism. It defines the maximum
         time to wait for IGMP/MLD report memberships on a port before removing
         the port from multicast group membership. The value is in units of
         tenths of a seconds. The range is from 0 to 31744."
    ::= { ipmcMvrConfigInterfaceEntry 7 }

ipmcMvrConfigInterfaceChannelProfile OBJECT-TYPE
    SYNTAX      DisplayString (SIZE(0..16))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "The profile used for the channel filtering condition in the specific
         MVR VLAN. Profile selected for designated interface channel is not
         allowed to have overlapped permit group address by comparing with other
         MVR VLAN interface's channel."
    ::= { ipmcMvrConfigInterfaceEntry 8 }

ipmcMvrConfigInterfaceAction OBJECT-TYPE
    SYNTAX      RowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "Action"
    ::= { ipmcMvrConfigInterfaceEntry 100 }

