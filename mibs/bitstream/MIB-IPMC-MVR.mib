ipmcMvrConfigVlanPortTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF IpmcMvrConfigVlanPortEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is a table for managing MVR port roles of a specific MVR VLAN
         interface."
    ::= { ipmcMvrConfig 5 }

ipmcMvrConfigVlanPortEntry OBJECT-TYPE
    SYNTAX      IpmcMvrConfigVlanPortEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
