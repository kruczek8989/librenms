synceControlSourcesClockSourceTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF SynceControlSourcesClockSourceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "This is the SyncE sources control structure."
    ::= { synceControlSources 1 }

synceControlSourcesClockSourceEntry OBJECT-TYPE
    SYNTAX      SynceControlSourcesClockSourceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "The sourceId index must be a value must be a value from 0 up to the
         number of sources minus one."
    INDEX       { synceControlSourcesClockSourceSourceId }
    ::= { synceControlSourcesClockSourceTable 1 }

SynceControlSourcesClockSourceEntry ::= SEQUENCE {
    synceControlSourcesClockSourceSourceId  Integer32,
    synceControlSourcesClockSourceClearWtr  Unsigned8
}

synceControlSourcesClockSourceSourceId OBJECT-TYPE
    SYNTAX      Integer32 (0..32767)
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION
        "-"
    ::= { synceControlSourcesClockSourceEntry 1 }

synceControlSourcesClockSourceClearWtr OBJECT-TYPE
    SYNTAX      Unsigned8
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION
        "-"
    ::= { synceControlSourcesClockSourceEntry 2 }

