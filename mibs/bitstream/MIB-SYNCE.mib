synceControlSourcesClockSourceInfoGroup OBJECT-GROUP
    OBJECTS     { synceControlSourcesClockSourceClearWtr }
    STATUS      current
    DESCRIPTION
        "A collection of objects suitable for bulk operations."
    ::= { synceMibGroups 10 }

synceMibCompliance MODULE-COMPLIANCE
    STATUS      current
    DESCRIPTION
        "The compliance statement for the implementation."

    MODULE      -- this module

    MANDATORY-GROUPS { synceCapabilitiesGlobalInfoGroup,
                       synceConfigGlobalClockSelectionModeInfoGroup,
                       synceConfigGlobalStationClocksInfoGroup,
                       synceConfigSourcesClockSourceNominationInfoGroup,
                       synceConfigPortsPortConfigInfoGroup,
                       synceStatusGlobalClockSelectionModeInfoGroup,
                       synceStatusSourcesClockSourceNominationInfoGroup,
                       synceStatusPortsPortStatusInfoGroup,
                       synceStatusPtpPortStatusInfoGroup,
                       synceControlSourcesClockSourceInfoGroup }

    ::= { synceMibCompliances 1 }

END
