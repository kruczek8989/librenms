-- *****************************************************************
-- MIB-PERF-MONITOR: BitStream ENT private MIB
-- ****************************************************************

MIB-PERF-MONITOR DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP FROM SNMPv2-CONF
    MODULE-IDENTITY, OBJECT-TYPE FROM SNMPv2-SMI
    TEXTUAL-CONVENTION FROM SNMPv2-TC
    switchMgmt FROM BTS-SMI
    Counter64 FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    MacAddress FROM SNMPv2-TC
    TruthValue FROM SNMPv2-TC
    DisplayString FROM BTS-TC
    InterfaceIndex FROM BTS-TC
    Unsigned16 FROM BTS-TC
    Unsigned8 FROM BTS-TC
    ;

perfMonitorMib MODULE-IDENTITY
    LAST-UPDATED "202503200000Z"
    ORGANIZATION
        "BitStream Sp. z o.o."
    CONTACT-INFO
        "www.bitstream.pl"
    DESCRIPTION
        "This is a private version of the Performance Monitor MIB."
    REVISION    "201410060000Z"
    DESCRIPTION
        "Initial version."
    ::= { switchMgmt 117 }


PerfMonitorEvcPortType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration indicates per port type for EVC statistics."
    SYNTAX      INTEGER { evcNni(0), evcUni(1) }

PerfMonitorMepDirection ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration indicates MEP instance direction measured by PM."
    SYNTAX      INTEGER { down(0), up(1) }

PerfMonitorMepDmTimeUnit ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration indicates time resolution of the DM results measured
         by PM."
    SYNTAX      INTEGER { us(0), ns(1) }

PerfMonitorMepRate ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration indicates MEP frame rate of CCM/LMM PDU which is the
         inverse of transmission period as described in Y.1731."
    SYNTAX      INTEGER { frames300PerSecond(1),
                          frames100PerSecond(2), frames10PerSecond(3),
                          frames1PerSecond(4), frames6PerMinute(5),
                          frames1PerMinute(6), frames6PerHour(7) }

PerfMonitorTransferMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration indicates PM transfer interval mode."
    SYNTAX      INTEGER { all(1), new(2), fixAmount(3) }

